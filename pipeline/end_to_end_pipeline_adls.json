			{
				"name": "fact_customer_rfm",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Orders",
						"dependencyConditions": [
							"Succeeded"
						]
					},
					{
						"activity": "Order_items",
						"dependencyConditions": [
